def f_to_c(f_temp, c_temp): 
  c_temp = (f_temp - 32) * 5/9
  return c_temp
  f100_in_celsius = f_to_c(100)
def c_to_f(c_temp, f_temp): 
  f_temp = (c_temp) * 9/5 + 32
  c0_in_fahrenheit = c_to_f(0)
def get_force(mass, acceleration):
  get_force = mass * acceleration
  return get_force 
  get_force(train_mass,train_acceleration)
  get_force = train_force
  print("The GE train supplies " + train_force + "Newtons of force.")
def get_energy(mass, c): 
  c = 3*10**8
  get_energy = mass*c*c
  return get_energy
  get_energy = bomb_mass(c)
  bomb_energy = get_energy
  print("A 1kg bomb supplies " + bomb_energy + "Joules")
def get_work(mass, acceleration, distance): 
  force = get_force * distance
  return get_work
